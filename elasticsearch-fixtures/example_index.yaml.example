# Exemple de fichier de fixtures pour un index Elasticsearch
# Nom du fichier d√©finit le nom de l'index (ici : example_index)

mapping:
  properties:
    id:
      type: keyword
    name:
      type: text
    age:
      type: integer
    created_at:
      type: date

data:
  - _id: "1"
    id: "user1"
    name: "John Doe"
    age: 30
    created_at: "2023-01-01T00:00:00Z"
  - _id: "2"
    id: "user2"
    name: "Jane Smith"
    age: 25
    created_at: "2023-02-01T00:00:00Z"